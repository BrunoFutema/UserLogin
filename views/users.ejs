<!DOCTYPE html>
<html lang="en">
<head>
    <link rel="stylesheet" href="lib/bootstrap/css/bootstrap.min.css">
    <style>
        .jumbotron {
            width: 600px;
            margin-left: auto;
            margin-right: auto;
            margin-top: 20px;
            text-align: center;
            background-color: rgba(40, 45, 80, 0.5);
        }
        h2 {
            margin-bottom: 50px;
        }
        button {
            cursor: pointer;
        }
    </style>
</head>
<body>
    <div class="jumbotron">
        <h2><%= title %></h2>
        <table class="table table-striped table-dark table-bordered table-hover">
            <thead>
                <tr>
                    <th>UserName</th>
                    <th>Email</th>
                    <th>Password</th>
                </tr>
            </thead>
            <tbody>
                <% users.forEach(function (user) { %>
                    <tr>
                        <td><%= user.username %></td>
                        <td><%= user.email %></td>
                        <td><%= user.password %></td>
                    </tr>
                <%})%>
            </tbody>
        </table>
        <button id="btnSair" type="button" class="btn btn-primary">
            Sair
        </button>
    </div>
 </ul>
 <script src="lib/jquery/jquery-3.2.1.min.js"></script>
 <script>
     $('#btnSair').on('click', function () {
         window.location.href = '/';
     });
 </script>
</body>
</html>